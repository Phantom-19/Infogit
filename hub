def infogit():
   cls()
   print(logo_git)
   try:
   	Nom =raw_input("\n\033[1;91m[\033[1;93m&\033[1;91m] \033[1;97mEntrer le nom d\'utilisateur github \033[1;91m: \033[1;96m")
   	if Nom == '': 
            print("\033[1;91m[\033[1;93m?\033[1;91m] \033[1;97mA plus man.")
            raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
            Menu()
   	re=get('https://api.github.com/users/'+Nom+'/repos')
   	a=json.loads(re.text)
   except Exception as ef:
         print("\n\033[1;91m[\033[1;93m!\033[1;91m]\033[1;94m Erreur %s"%(ef))
         raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
         Menu()
   items = []
   try:
   	for item in a:
   	    items.append(item['name'])
   except TypeError: 
       print("\033[1;91m[\033[1;93mx\033[1;91m]\033[1;97m Le nom d\'utilisateur\033[1;91m",Nom,"\033[1;97m n\'existe pas.")
       raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
       Menu()
   s=len(items)
   if s <= 0:
           print("\033[1;91m[\033[1;97m!\033[1;91m]\033[1;97m Le nom d\'utilisateur\033[38;5;145m",Nom,"\033[1;97m n\'a pas de referentiel.")
           raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
           Menu()
   elif s > 0:                  
           print("\033[1;91m[\033[1;93m*\033[1;91m] \033[1;97mIl y\'a\033[1;91m",s,"\033[1;97mréférentiels sur le github de\033[1;96m",Nom,"\033[1;91m[\033[1;93m*\033[1;91m]\n")
           print("\033[1;96m  __ ___ ___    _     _        _ ")
           print("\033[1;96m /__  |   |    /  |  / \ |\ | |_ ")
           print("\033[1;96m \_| _|_  |    \_ |_ \_/ | \| |_\n")
   c=int(1)
   while c < s:
   	for i in range(s):                             
              time.sleep(1)
              print ("\t\033[1;91m[\033[1;93m"+str(c)+"\033[1;91m]\033[1;97m Clone\033[1;92m",items[i])
              c+=1
   try:
   	Dmd=int(input("\n\033[1;91m[\033[1;93m*\033[1;91m] \033[1;97mClonage du lien git\033[1;91m (\033[1;95mnuméro\033[1;91m) :\033[1;96m "))       
   	if Dmd <= 0:
               print("\033[1;91m[\033[1;93m?\033[1;91m] \033[1;97mA plus\n") 
               raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
               Menu()
   	os.system('git clone https://github.com/'+Nom+'/'+items[Dmd-1])
   	sp.call('mv '+str(items[Dmd-1])+' ..',shell=True,stdout=sp.DEVNULL, stderr=sp.STDOUT)
   	print("\n\033[1;91m[\033[1;93m@\033[1;91m]\033[1;92m Installation de \033[1;96m",items[Dmd-1],"\033[1;92mreussie\033[1;92m.")
   	mael("\n\033[1;91m[\033[1;93m#\033[1;91m]\033[1;97m Verifier l\'emplacement du dossier en tapant \033[1;95m cd\033[1;97m. \033[1;91m[\033[1;93m#\033[1;91m]\n\033[1;95m\033[1;91m[\033[1;93m#\033[1;91m] cd\033[1;97m pour quitter le dossier courant.              \033[1;91m[\033[1;93m#\033[1;91m]\n\033[1;91m[\033[1;93m#\033[1;91m]\033[1;97m Ensuite taper\033[1;95m cd\033[1;91m +\033[1;97m le dossier.Puis taper\033[1;93m ls\033[1;97m.     \033[1;91m[\033[1;93m#\033[1;91m]\n ")
   except KeyboardInterrupt: 
        print("\033[1;91m[\033[1;93m*\033[1;91m] Interruption\033[1;91m] : \033[1;94m]Arret")
        raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
        Menu()
   except Exception as F:
        print("\033[1;91m[\033[1;97mA plus\033[1;91m] %s"%(F))
        raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
        Menu()
   print("\033[1;97m╔═════════════════════════════════════════════╗")
   print("\033[1;97m║\033[1;91m[\033[1;93m**\033[1;91m]\033[38;5;95m      Developpeur \033[1;97m:\033[38;5;214m  Faxel           \033[1;91m[\033[1;93m**\033[1;91m]\033[1;97m║")
   print("\033[1;97m╚═════════════════════════════════════════════╝")
   mael("\n             \033[1;91m[\033[1;93m**\033[1;91m] \033[38;5;214mFaxel\033[1;97m un jour,\033[38;5;214mFaxel \033[1;97mtoujours \033[1;91m[\033[1;93m**\033[1;91m] \n")
   raw_input('\n\033[1;91m [\033[1;93m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu...')
